<topology asn="100">
  <!-- Internal routers -->
  <node internal="true" name="A"></node>
  <node internal="true" name="B"></node>
  <node internal="true" name="C"></node>
  <node internal="true" name="D"></node>
  <node internal="true" name="E"></node>
  <edge source="A" target="C"></edge>
  <edge source="C" target="D"></edge>
  <edge source="D" target="E"></edge>
  <edge source="E" target="B"></edge>

  <edge source="A" target="peer"></edge>
  <edge source="B" target="peer"></edge>
  <!--External Peers -->
  <node internal="false" asn="200" name="peer"></node>
</topology>
